******************************************************************
* DEFINE 'MFLD' STATEMENTS FOR "MOD" HERE
******************************************************************
LE       MSG   TYPE=OUTPUT,NXT=LE2,OPT=1,SOR=(LOC,IGNORE)
         SEG
         MFLD  S,LTH=6
         MFLD  SL,LTH=2
         MFLD  YF,LTH=2
         MFLD  YS,LTH=2
         MFLD  (TRAN,'LOCEMP')
         MSGEND
******************************************************************
* DEFINE 'MFLD' STATEMENTS FOR "MID" NEXT
******************************************************************
LE2      MSG   TYPE=INPUT,SOR=(LOC),NXT=LE3
         SEG
         MFLD  TRAN,LTH=8,FILL=C' '
         MFLD  S,LTH=6
         MFLD  SL,LTH=2,JUST=R,FILL=C'0'
         MFLD  YF,LTH=2,JUST=R,FILL=C'0'
         MFLD  YS,LTH=2,JUST=R,FILL=C'0'
         MFLD  'STUFF',LTH=5
         MSGEND
******************************************************************
* DEFINE 'MFLD' STATEMENTS FOR "MOD" HERE
******************************************************************
